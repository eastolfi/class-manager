    "core-js": "^3.4.7",
    "cors": "^2.8.5",
    "docx": "^5.0.0-rc7",
    "easy-pie-chart": "^2.1.7",
    "express": "^4.17.1",
    "firebase": "^7.13.1",
    "flag-icon-css": "^3.5.0",
    "rxjs": "~6.5.3",
    "screenfull": "^4.2.0",
    "tslib": "^2.0.0",
