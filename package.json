{
  "name": "class-manager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.es": "ng serve -c es",
    "build": "ng build",
    "build.prod": "ng build -c production",
    "build.prod.es": "ng build -c production-es",
    "deploy": "firebase deploy --only hosting:class-manager",
    "deploy.es": "firebase deploy --only hosting:test",
    "build.and.deploy": "rm -rf dist/ && npm run build.prod && npm run deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "loc": "ng xi18n --output-path src/locale",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/es.json ./src/assets/i18n/fr.json --clean --sort --format namespaced-json --marker _"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "~10.2.5",
    "@angular/common": "~10.2.0",
